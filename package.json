{
  "name": "any-command",
  "displayName": "any-command",
  "description": "想要什么自己实现",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "lv",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "any-command.openInIDEA",
        "title": "在 IDEA 中打开当前项目"
      },
      {
        "command": "any-command.openInTerminal",
        "title": "在 Terminal 中打开当前项目"
      },
      {
        "command": "any-command.openInExplorer",
        "title": "在 explorer 里打开当前项目"
      },
      {
        "command": "any-command.openGitGraph",
        "title": "打开git graph"
      }
    ],
    "keybindings": [
      {
        "command": "any-command.openGitGraph",
        "key": "alt+f2"
      },
      {
        "command": "any-command.openGitGraph",
        "key": "cmd+f2"
      }
    ]
  },
  "scripts": {
    "package": "vsce package --no-yarn",
    "compile": "webpack",
    "watch": "webpack --watch",
    "build-package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "vsce": "^2.15.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  }
}
